<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextViewReadMore">
        <attr name="android:text"/>
        <attr name="readMoreMaxLines" format="integer"/>
        <attr name="collapsed" format="boolean"/>
        <attr name="expandText" format="string"/>
        <attr name="expandTextColor" format="reference|color"/>
        <attr name="expandTextUnderline" format="boolean"/>
        <attr name="expandTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="collapseText" format="string"/>
        <attr name="collapseTextColor" format="reference|color"/>
        <attr name="collapseTextUnderline" format="boolean"/>
        <attr name="collapseTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="actionClickColor" format="reference|color"/>
        <attr name="ellipsisType">
            <flag name="dots" value="0"/>
            <flag name="none" value="1"/>
        </attr>
        <attr name="duration"/>
        <attr name="durationExpand" format="integer" min="0"/>
        <attr name="durationCollapse" format="integer" min="0"/>
        <attr name="interpolator" format="integer">
            <flag name="decelerate" value="0"/>
            <flag name="accelerate" value="1"/>
            <flag name="anticipate_overshoot" value="2"/>
            <flag name="anticipate" value="3"/>
            <flag name="bounce" value="4"/>
            <flag name="fast_out_linear_in" value="5"/>
            <flag name="fast_out_slow_in" value="6"/>
            <flag name="linear_out_slow_in" value="7"/>
        </attr>
        <attr name="interpolatorExpand" format="integer">
            <flag name="decelerate" value="0"/>
            <flag name="accelerate" value="1"/>
            <flag name="anticipate_overshoot" value="2"/>
            <flag name="anticipate" value="3"/>
            <flag name="bounce" value="4"/>
            <flag name="fast_out_linear_in" value="5"/>
            <flag name="fast_out_slow_in" value="6"/>
            <flag name="linear_out_slow_in" value="7"/>
        </attr>
        <attr name="interpolatorCollapse" format="integer">
            <flag name="decelerate" value="0"/>
            <flag name="accelerate" value="1"/>
            <flag name="anticipate_overshoot" value="2"/>
            <flag name="anticipate" value="3"/>
            <flag name="bounce" value="4"/>
            <flag name="fast_out_linear_in" value="5"/>
            <flag name="fast_out_slow_in" value="6"/>
            <flag name="linear_out_slow_in" value="7"/>
        </attr>
    </declare-styleable>

</resources>